    #landing-page {
        display: grid;
        height: 100vh;
        width: 100vw;
        grid-template-columns: 1fr 1fr 1fr;
        grid-template-rows: 1fr 1fr 1fr;
        background: url(/landing-image.png) center center fixed no-repeat;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
        overflow: hidden;
    }

    #landing-page > div:nth-child(1) {
      grid-column: 1/2;
      grid-row: 1/2;

      font-family: Roboto;
      font-style: normal;
      font-weight: bold;
      font-size: 15px;
      line-height: 18px;
      color: #6FCF97;
      padding: 2rem;

      margin-right: -15rem;
    }

    #landing-page > div:nth-child(2) {
      grid-column: 2/-1;
      grid-row: 1/2;
      background: #6FCF97;
      height: fit-content;
      margin-left: auto;
      padding: 1rem 2rem;
      margin-right: 2rem;
      margin-top: 2rem;
      border-radius: 2px;

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 20px;
      line-height: 29px;
      text-align: center;
      color: #251e1e;

      align-items: center;
      cursor: pointer;
    }

    #landing-page > div:nth-child(2):hover {
      background: #6FCF87;
    }

    .cta > svg {
      margin: 1rem 0;
    }

    #landing-page > div:nth-child(3) {
      grid-column: 1/2;
      grid-row: 2/3;
      display: flex;
      flex-direction: column;
      justify-content: center;
      padding-left: 2rem;
    }

    .sliders > svg {
      cursor: pointer;
      margin: 4px 0;
    }

    #landing-page > div:nth-child(4) {
      grid-column: 2/3;
      grid-row: 2/3;

      display: flex;
      flex-direction: column;
      justify-content: center;

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      line-height: 60px;
      text-align: center;
      color: #E0E0E0;

      white-space: nowrap;

    }

    #landing-page > div:nth-child(5) {
      grid-column: 3/-1;
      grid-row: 2/3;
      display: flex;
      flex-direction: column;
      justify-content: center;
      margin-left: auto;
      margin-right: 2rem;
      align-items: center;
    }

    .social-media-icons > svg {
      cursor: pointer;
      margin: 1rem 0;
    }


    #landing-page > div:nth-child(6) {
      grid-column: 1/2;
      grid-row: 3/-1;

      display: flex;
      flex-direction: column;
      justify-content: flex-end;
      padding-bottom: 2rem;
      padding-left: 2rem;

      margin-right: -15rem;

    }

    .photo-credits > p {
      transform-origin: left;
      transform: rotate(270deg);
      /* Photo by David Monje on Unsplash */
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 9px;
      line-height: 11px;

      /* Gray 4 */
      color: #BDBDBD;

    }


    #landing-page > div:nth-child(7) {
      grid-column: 2/3;
      grid-row: 3/-1;

      display: flex;
      justify-content: flex-end;
      flex-direction: column;
      align-items: center;
      padding-bottom: 2rem;
    }

    #landing-page > div:nth-child(8) {
      grid-column: 3/-1;
      grid-row: 3/-1;

      display: flex;
      justify-content: flex-end;
      padding: 0 2rem 2rem 0;
      margin-left: -20rem;
    }

